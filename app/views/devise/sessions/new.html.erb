<%#= render "devise/shared/links" %>

<airticle class="card-body mx-auto">
  <h4 class="card-title mt-3 text-center">新規登録</h4>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <!--    <p class="divider-text">-->
    <!--      <span class="bg-white">または</span>-->
    <!--    </p>-->
    <div class="form-group input-group">
      <div class="input-group-prepend">
      </div>
      <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder:"email" %>
    </div>
    <div class="form-group input-group">
      <div class="input-group-prepend">
      </div>
      <%= f.password_field :password, autocomplete: "password", placeholder:"パスワード" %>
    </div>
    <div class="form-group input-group">
      <div class="form-group">
      </div>
    <% if devise_mapping.rememberable? %>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
    <% end %>
    <div class="form-group input-group">
      <div class="form-group">
      </div>
        <%= f.submit "Log in", class: "btn btn-lg btn-primary btn-block"%>
    </div>
  <p class="text-center">アカウントをお持ちですか？<%= link_to "サインアップする", new_user_registration_path %></p>
  <!--    <p class="mt-5 mb-3 text-muted">&copy; 2019</p>-->
  <%#= render "devise/shared/links" %>
  <% end %>
</airticle>
